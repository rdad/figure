{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "outDir": "dist/",
        "sourceMap": true
    },
    "exclude": [
        "node_modules",
        "dist",
        "typings"
    ]
}